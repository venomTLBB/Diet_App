<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".MainActivity"
    android:background="@color/colorBaseBackGround"
    tools:showIn="@layout/activity_main">

    <Button
        android:id="@+id/mm_b_dailyTracker"
        android:layout_width="145dp"
        android:layout_height="175dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="15dp"
        android:text="@string/mm_b_dailyTracker" />

    <Button
        android:id="@+id/mm_b_dietDictionary"
        android:layout_width="175dp"
        android:layout_height="175dp"
        android:layout_below="@+id/mm_b_dailyTracker"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="210dp"
        android:text="@string/mm_b_dietDictionary" />

    <Button
        android:id="@+id/mm_b_restaurants"
        android:layout_width="175dp"
        android:layout_height="175dp"
        android:layout_below="@+id/mm_b_dietDictionary"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="210dp"
        android:text="@string/mm_b_restaurants" />

    <Button
        android:id="@+id/mm_b_recipes"
        android:layout_width="175dp"
        android:layout_height="175dp"
        android:layout_below="@+id/mm_b_dailyTracker"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="210dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="15dp"
        android:text="@string/mm_b_recipes" />

    <Button
        android:id="@+id/mm_b_profile"
        android:layout_width="175dp"
        android:layout_height="175dp"
        android:layout_below="@+id/mm_b_recipes"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="210dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="15dp"
        android:text="@string/mm_b_profile" />

</RelativeLayout>